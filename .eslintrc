{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {},
  "rules": {
    "max-len": [2, 200, 4],
    "no-param-reassign": 0,
    "indent": [1, 2, {
      "SwitchCase": 1
    }],
    "no-console": 0,
    "object-curly-spacing": 0,
    "prefer-template": 1,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "import/no-dynamic-require": 0
  },
  "plugins": [
    "import"
  ]
}
